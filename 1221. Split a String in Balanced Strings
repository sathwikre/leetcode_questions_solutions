class Solution {
    public int balancedStringSplit(String s) {
        return find(s);
    }
    static int find(String s){
        int c=0;
        int bal=0;
        for(int i=0;i<s.length();i++){
            if(bal==0){
                c++;
            }
            if(s.charAt(i)=='L'){
                bal++;
            }else{
                bal--;
            }

        }
        return c;
    }
}
