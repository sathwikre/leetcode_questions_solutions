class Solution {
    public int minOperations(String[] logs) {
        return find(logs);
    }
    static int find(String[] arr){
        int c=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i].equals("../")){
                c--;
                if(c<0){
                    c=0;
                }
            }
            else if(arr[i].equals("./")){
                continue;
            }else{
                c++;
            }
        }
        if(c<0){
            return 0;
        }
        return c;
    }
}
