class Solution {
    public int maxSubArray(int[] nums) {
        return find(nums);
    }
    static int find(int[] nums){
        if(nums.length<=1){
            return nums[0];
        }
        int sum=0;
        int maxsum=nums[0];
        for(int i=0;i<nums.length;i++){
            sum+=nums[i];
            maxsum=Math.max(maxsum,sum);
            if(sum<0){
                sum=0;
            }
        }
        return maxsum;
    }
}
