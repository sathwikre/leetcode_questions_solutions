class Solution {
    public int[] decompressRLElist(int[] nums) {
        return find(nums);
    }
    static int[] find(int[] arr){
        List<Integer> ans=new ArrayList<>();
        for(int i=0;i<arr.length;i=i+2){
            int freq=arr[i];
            int val=arr[i+1];
            for(int j=0;j<freq;j++){
                ans.add(val);
            }
        }
        int[] num=new int[ans.size()];
        for(int i=0;i<ans.size();i++){
            num[i]=ans.get(i);
        }
        return num;
    }
}
