class Solution {
    public int maxProduct(int n) {
        String s=n+"";
        int ss=s.length();
        int[] arr=new int[ss];
        int i=0;
        while(n>0){
            arr[i]=n%10;
            n=n/10;
            i++;
        }
        Arrays.sort(arr);
        return find(arr);
    }
    static int  find(int[] arr){
        return arr[arr.length-1]*arr[arr.length-2];
    }
}
