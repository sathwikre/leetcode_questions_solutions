class Solution {
    public int minimumBoxes(int[] apple, int[] capacity) {
        return find(apple,capacity);
    }
    static int find(int[] arr,int[] cap){
        int sum=0;
        for(int i=0;i<arr.length;i++){
            sum+=arr[i];
        }
        Arrays.sort(cap);
        int c=0;
        for(int i=cap.length-1;i>=0;i--){
            sum-=cap[i];
            c++;
            if(sum<0){
                return c;
            }
        }
        return c;
    }
}
