class Solution {
    public int accountBalanceAfterPurchase(int purchaseAmount) {
        return find(purchaseAmount);
    }
    static int find(int n){
        if(n>=0&&n<5){
            return 100;
        }
        if((n>=5&&n<=10)||(n>=11&&n<15)){
            return 100-10;
        }
        if((n>=15&&n<=20)||(n>=21&&n<25)){
            return 100-20;
        }
        if((n>=25&&n<=30)||(n>=31&&n<35)){
            return 100-30;
        }if((n>=35&&n<=40)||(n>=41&&n<45)){
            return 100-40;
        }if((n>=45&&n<=50)||(n>=51&&n<55)){
            return 100-50;
        }if((n>=55&&n<=60)||(n>=61&&n<65)){
            return 100-60;
        }if((n>=65&&n<=70)||(n>=71&&n<75)){
            return 100-70;
        }if((n>=75&&n<=80)||(n>=81&&n<85)){
            return 100-80;
        }if((n>=85&&n<=90)||(n>=91&&n<95)){
            return 100-90;
        }if(n>=95&&n<=100){
            return 100-100;
        }
        return 0;
    }
}
