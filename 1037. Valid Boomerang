class Solution {
    public boolean isBoomerang(int[][] points) {
       return find(points) ;
    }
    static boolean find(int[][] arr){
        int x1=arr[0][0];
        int y1=arr[0][1];
        int x2=arr[1][0];
        int y2=arr[1][1];
        int x3=arr[2][0];
        int y3=arr[2][1];
        int area=Math.abs(x1*(y2-y3)+x2*(y3-y1)+x3*(y1-y2));
        return area!=0;
    }
}
