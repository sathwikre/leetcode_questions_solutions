class Solution {
    public int majorityElement(int[] arr) {
       HashMap<Integer,Integer> hash=new HashMap<>();
       for(int i=0;i<arr.length;i++){
         hash.put(arr[i], hash.getOrDefault(arr[i], 0) + 1);
       }
       int ans=arr.length/2;
       int a=0;
       for(int i=0;i<arr.length;i++){
        if(hash.get(arr[i])>ans){
            ans=hash.get(arr[i]);
            a=arr[i];
        }
       }
       return a;
    }
}
