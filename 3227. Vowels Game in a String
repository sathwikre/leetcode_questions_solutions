class Solution {
    public boolean doesAliceWin(String s) {
        return find(s);
    }
    static boolean find(String s){
        int c=0;
        for(int i=0;i<s.length();i++){
            if(isvowel(s.charAt(i))){
                c++;
            }
        }
       return get(c);
    }
    static boolean isvowel(char ch){
        return ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u';
    }
    static boolean get(int c){
         if(c==0){
            return false;
        }
        if(c%2!=0){
            return true;
        }
        if(c%2==0){
            return get(c-1);
        }
        return true;
    }
}
