class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        return find(matrix,target);
    }
    static boolean find(int[][] arr,int target){
        for(int i=0;i<arr.length;i++){
            for(int j=0;j<arr[i].length;j++){
                if(arr[i][j]==target){
                    return true;
                }
            }
        }
        return false;
    }
}
