class Solution {
    public int unequalTriplets(int[] nums) {
        return find(nums);
    }
    static int find(int[] arr){
           int c=0;
           int n=arr.length-1;
        for(int i=0;i<=n;i++){
            for(int j=0;j<=n;j++){
                for(int k=0;k<=n;k++){
                    if(i<j&&j<k&&arr[i]!=arr[j]&&arr[j]!=arr[k]&&arr[i]!=arr[k]){
                        c++;
                    }
                }
            }
        }
        return  c;
    }
}
