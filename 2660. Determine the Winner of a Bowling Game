class Solution {
    public int isWinner(int[] player1, int[] player2) {
        return find(player1,player2);
    }
    static int find(int[] arr1,int[] arr2){
        int s1=0;
        int s2=0;
        for(int i=0;i<arr1.length;i++){
            if((i >= 1 && arr1[i - 1] == 10) || (i >= 2 && arr1[i - 2] == 10)){
               
                s1+=2*arr1[i];
            }else{
                s1+=arr1[i];
            }
        }
        for(int i=0;i<arr2.length;i++){
            if((i >= 1 && arr2[i - 1] == 10) || (i >= 2 && arr2[i - 2] == 10)){
                s2+=2*arr2[i];
            }else{
                s2+=arr2[i];
            }
        }
        if(s1>s2){
            return 1;
        }
        if(s1<s2){
            return 2;
        }
        else{
            return 0;
        }
    }
}
