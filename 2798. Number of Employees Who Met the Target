class Solution {
    public int numberOfEmployeesWhoMetTarget(int[] hours, int target) {
       return find(hours,target) ;
    }
    static int find(int[] arr,int t){
        int c=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]>=t){
                c++;
            }
        }
        return c;
    }
}
