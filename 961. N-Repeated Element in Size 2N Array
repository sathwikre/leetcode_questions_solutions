class Solution {
    public int repeatedNTimes(int[] nums) {
        Arrays.sort(nums);
        return find(nums);
    }
    static int  find(int[] arr){
        for(int i=1;i<arr.length;i++){
            if(arr[i]==arr[i-1]){
                return arr[i];
            }
        }
        return 0;
    }
}
