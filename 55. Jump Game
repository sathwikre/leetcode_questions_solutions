class Solution {
    public boolean canJump(int[] nums) {
        return find(nums);
    }
    static boolean find(int[] arr){
        if(arr.length<=1) return true;
        int maxlen=0;
        for(int i=0;i<arr.length;i++){
             if(i>maxlen){
                return false;
            }
            maxlen=Math.max(maxlen,i+arr[i]);
           
        }
        return true;
    }
}
