class Solution {
    public int[] diStringMatch(String s) {
        return find(s);
    }

    static int[] find(String s) {
        int n = s.length();
        int[] ans = new int[n + 1];
        int low = 0, high = n;

        for (int j = 0; j < n; j++) {
            if (s.charAt(j) == 'I') {
                ans[j] = low;
                low++;
            } else {
                ans[j] = high;
                high--;
            }
        }
        ans[n] = low;  
        return ans;
    }
}
