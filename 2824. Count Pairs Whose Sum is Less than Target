class Solution {
    public int countPairs(List<Integer> nums, int target) {
        return find(nums,target);
    }
    static int find(List<Integer> arr,int t){
        int c=0;
        for(int i=0;i<arr.size();i++){
            for(int j=i+1;j<arr.size();j++){
                if(i<j&&arr.get(i)+arr.get(j)<t){
                    c++;
                }
            }
        }
        return c;
    }
}
