class Solution {
    public int[] findColumnWidth(int[][] grid) {
       return find(grid) ;
    }
    static int[] find(int[][] arr){
        int[] ans=new int[arr[0].length];
        for(int i=0;i<arr.length;i++){
            for(int j=0;j<arr[0].length;j++){
                String s=String.valueOf(arr[i][j]);
                if(ans[j]<s.length()){
                    ans[j]=s.length();
                }
            }
        }
        return ans;
    }
}
