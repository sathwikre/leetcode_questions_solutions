class Solution {
    public boolean checkZeroOnes(String s) {
        return find(s);
    }
    static boolean find(String s){
        int c1=0;
        int c2=0;
        int max1=0;
        int max2=0;
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            if(ch=='1'){
                c1++;
            }else{
                max1=Math.max(max1,c1);
                c1=0;
            }
             if(ch=='0'){
                c2++;
            }else{
                max2=Math.max(max2,c2);
                c2=0;
            }
        }
        max1=Math.max(max1,c1);
        max2=Math.max(max2,c2);
        return max1>max2;
    }
}
