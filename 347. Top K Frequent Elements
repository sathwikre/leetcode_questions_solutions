import java.util.*;

class Solution {
    public int[] topKFrequent(int[] nums, int k) {
        HashMap<Integer, Integer> map = new HashMap<>();
        for (int n : nums) {
            map.put(n, map.getOrDefault(n, 0) + 1);
        }

        int[] ans = new int[k];
        int j = 0;
        for (int i = nums.length; i > 0 && j < k; i--) {
            for (Map.Entry<Integer, Integer> entry : map.entrySet()) {
                if (entry.getValue() == i) {
                    ans[j++] = entry.getKey();
                    if (j == k) break; 
                }
            }
        }

        return ans;
    }
}
