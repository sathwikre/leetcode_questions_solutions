class Solution {
    public int countElements(int[] nums) {
        return find(nums);
    }
    static int find(int[] arr){
        int max=arr[0];
        int min=arr[0];
        for(int i=1;i<arr.length;i++){
            if(arr[i]>max){
                max=arr[i];
            }
            if(arr[i]<min){
                min=arr[i];
            }
        }
        if(max==min){
            return 0;
        }
        int m=0;
        int n=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]==max){
                m++;
            }
            if(arr[i]==min){
                n++;
            }
        }
        return arr.length-m-n;
    }
}
