class Solution {
    public boolean findSubarrays(int[] nums) {
       return find(nums) ;
    }
    static boolean find(int[] arr){
        ArrayList<Integer> a=new ArrayList<>();
        for(int i=0;i<arr.length-1;i++){
            
                int sum=arr[i]+arr[i+1];
                if(a.contains(sum)){
                    return true;
                }else{
                    a.add(sum);
                }
            
        }
        return false;
    }
}
