class Solution {
    public int countLargestGroup(int n) {
        return find(n);
    }
    static int find(int n){
        int[] sums=new int[37];
        for(int i=1;i<=n;i++){
            sums[digitsum(i)]++;
        }
        int maxi = 0, count = 0;
        for (int i : sums) {
            if (i > maxi) {
                maxi = i;
                count = 1;
            } else if (i == maxi) {
                count++;
            }
        }
        return count;

    }
    static int digitsum(int n){
        int sum=0;
        while(n>0){
            int rem=n%10;
            n=n/10;
            sum+=rem;
        }
        return sum;
    }
}
