class Solution {
    public int findComplement(int num) {
        return find(num);
    }
    static int find(int n){
        String s=Integer.toBinaryString(n);
        char[] arr=s.toCharArray();
        for(int i=0;i<arr.length;i++){
            if(arr[i]=='1'){
                arr[i]='0';
            }else{
                arr[i]='1';
            }
        }
        String result = new String(arr);
         int number = Integer.parseInt(result, 2);
         return number;
    }
}
