class Solution {
    public boolean isOneBitCharacter(int[] bits) {
        return find(bits);
    }
    static boolean find(int[] arr){
        boolean ans=false;
        int i=0;
        while(i<arr.length){
            if(arr[i]==1){
                if(i<arr.length-1){
                    i+=2;
                    ans=false;
                }else{
                    i=i+1;
                    ans=false;
                }
            }else{
                ans=true;
                i=i+1;
            }
        }
        return ans;
    }
}
