class Solution {
    public int areaOfMaxDiagonal(int[][] dimensions) {
        return find(dimensions);
    }
    static int find(int[][] arr){
        int max=0;
        double area=-1;
        for(int i=0;i<arr.length;i++){
            double a=Math.sqrt(arr[i][0]*arr[i][0]+arr[i][1]*arr[i][1]);
             int currArea = arr[i][0] * arr[i][1];

            if (a > area || (a == area && currArea > max)) {
                area = a;
                max = currArea;
            }
        }
        return max;
    }
}
