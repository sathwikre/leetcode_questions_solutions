class Solution {
    public int[] decode(int[] encoded) {
        return find(encoded);
    }
    static int[] find(int[] arr){
        int[] ans=new int[arr.length+1];
        int x=0;
        for(int i=1;i<=ans.length;i++){
            x=x^i;
        }
        ans[0]=x;
        for(int i=0;i<arr.length;i++){
            if(i%2!=0){
                ans[0]=ans[0]^arr[i];
            }
        }
        for(int i=1;i<ans.length;i++){
            ans[i]=ans[i-1]^arr[i-1];
        }
        return ans;
    }
}
