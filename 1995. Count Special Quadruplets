class Solution {
    public int countQuadruplets(int[] nums) {
       return find(nums) ;
    }
     static int find(int[] arr){
           int c=0;
           int n=arr.length-1;
        for(int i=0;i<=n;i++){
            for(int j=0;j<=n;j++){
                for(int k=0;k<=n;k++){
                    for(int a=0;a<=n;a++){
                    if(i<j&&j<k&&k<a&&arr[i]+arr[j]+arr[k]==arr[a]){
                        c++;
                    }
                    }
                    
                }
            }
        }
        return  c;
    }
}
