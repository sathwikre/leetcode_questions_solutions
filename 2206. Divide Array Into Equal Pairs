class Solution {
    public boolean divideArray(int[] nums) {
        return find(nums);
    }
    static boolean find(int[] arr){
        int max=-1;
        for(int i=0;i<arr.length;i++){
            if(max<arr[i]){
                max=arr[i];
            }
        }
        int[] hash=new int[max+1];
        for(int i=0;i<arr.length;i++){
            hash[arr[i]]++;
        }
        for(int i=0;i<hash.length;i++){
            if(hash[i]%2!=0){
                return false;
            }
        }
        return true;
    }
}
